//-
  Layout based on SB-Admin 2 template for Bootstrap.
  https://github.com/IronSummitMedia/startbootstrap/tree/master/templates/sb-admin-v2
doctype 5
html
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')

    title= title


    block css
      //- Core CSS
      link(rel='stylesheet', href='/css/bootstrap.css')
      link(rel='stylesheet', href='/font-awesome/css/font-awesome.css')

      //- Template CSS
      link(rel='stylesheet', href='/css/sb-admin.min.css')

      //- Page CSS
      link(rel='stylesheet', href='/css/style.min.css')

  body
    #wrapper
      nav.navbar.navbar-default.navbar-fixed-top(role='navigation', style='margin-bottom: 0')
        
        .navbar-header
          button.navbar-toggle(type='button', data-toggle='collapse', data-target='.sidebar-collapse')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='/') MediaGraph
        
          .navbar-default.navbar-static-side(role='navigation')
            .sidebar-collapse
            ul#side-menu.nav
              li.sidebar-search
                .input-group.custom-search-form
                  input.form-control(type='text', placeholder='Search...')
                  span.input-group-btn
                    button.btn.btn-default(type="button")
                      i.fa.fa-search
              block sidebar-links

      #page-wrapper
        block content
  
  //- Javascript
  block scripts
    //- Core scripts
    script(src='/js/jquery-1.10.2.js')
    script(src='/js/bootstrap.min.js')
    script(src='/js/plugins/metisMenu/jquery.metisMenu.js')

    //- Template scripts
    script(src='/js/sb-admin.js')

    //- Page scripts

    